package view;

public class Prueba {
	public static void main(String[] args) {
	class C1{
		C1(){
			System.out.print("c1");
		}
	}
	class C2 extends C1{
		C2(int a){
			System.out.print("c2 con a");
		}
	}
	class C3 extends C2{
		C3(){//error de compilación porque no existe constructor sin parametros en C2
			System.out.print("c3");
		}
	}

	//Y en algún main

	C3 a=new C3(); 
	class C1{
		C1(){
			System.out.print("c1");
		}
	}
	class C2 extends C1{
		
	}
	class C3 extends C2{
		C3(){
			System.out.print("c3");
		}
	}

	//Y en algún main

	C3 a=new C3(); //c1c3 
	
	class C1{
		C1(){
			System.out.print("c1");
		}
	}
	class C2 extends C1{
		C2(int a){
			System.out.print("c2 con a");
		}
		C2(){
			this(1);
		}
	}
	class C3 extends C2{
		C3(){
			System.out.print("c3");
		}
	}

	//Y en algún main

	C3 a=new C3(); //c1c2 con ac3
	
	}

}
