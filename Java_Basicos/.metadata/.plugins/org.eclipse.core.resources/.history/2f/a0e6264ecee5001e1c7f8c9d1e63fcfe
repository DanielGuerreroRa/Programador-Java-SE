package principal;

public class Comprobar {

	public static void main(String[] args) {
		int num=5;
		//si el numero es par nos muestra la mitad de este n√∫mero
		//pero si es impar muestra la mitad del anterior
		if(num %2 ==0 ) {
			System.out.println(num/2);
		}else { 
			System.out.println(--num/2);
		}
			
		
			
		}
	}
	


