package principal;

public class Factorial {

	public static void main(String[] args) {
		int num=7;
		int factorial = 1;
		//Este programa calcula y muestra el factorial de un nÃºmero
		for(int i = num; i>2 ; i--) {
			factorial=factorial*i;
		}
		System.out.println("El factorial del numero " + num + " es: " + factorial);


	}

}
