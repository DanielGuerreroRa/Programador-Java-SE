package principal;

public class ComprobarPrimo {

	public static void main(String[] args) {
		int num=17;
		boolean primo =true;
		//Indica si el número es primo o no primo
		for(int i = 2; i<num; i++) {
			if(num % 1 == 0) {
				primo = false;
				
			}
		}
		System.out.println("El número " + num + "es primo? " + primo);

	}

}
