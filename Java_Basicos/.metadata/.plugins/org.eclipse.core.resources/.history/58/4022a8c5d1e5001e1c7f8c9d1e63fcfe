package principal;

public class Division {

	public static void main(String[] args) {
		int x=8;
		int y=15;
		//mostrar el resultado de dividir mayor entre menor
		//respetando cifras decimales
		double rs;
		
		if(x>y) {
			rs=(double)x/y;
		}else {
			rs=(double)y/x;
		}
		System.out.println(rs);

	}

}
