package principal;

public class Factorial {

	public static void main(String[] args) {
		int num=7;
		int resultado = 0;
		//Este programa calcula y muestra el factorial de un n√∫mero
		for(int i = 1; i<num ; i++) {
			resultado=i*num;
		}
		System.out.println("El resultado es: " + resultado);


	}

}
