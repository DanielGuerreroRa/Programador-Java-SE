package principal;

public class ComprobarPrimo {

	public static void main(String[] args) {
		int num=16;
		boolean primo =true;
		//Indica si el n√∫mero es primo o no primo
		for(int i = 2; i<num; i++) {
			if(num % 1 == 0) {
				primo = false;
				break;
			}
			if(primo) {
				System.out.println("Es primo");
			}else {
				System.out.println("No es primo");
			}
			
			
		
		}
		

	}

}
